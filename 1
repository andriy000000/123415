result = []

def dilen(a, b):
    try:
        if a < b:
            raise ValueError("a повинно бути більше або рівне b")
        if b == 0:
            raise ZeroDivisionError("b не повинно бути нулем")
        if b > 100:
            raise IndexError("b не повинно перевищувати 100")
        return a / b
    except ValueError as ve:
        print(f"ValueError: {ve}")
    except ZeroDivisionError as zde:
        print(f"ZeroDivisionError: {zde}")
    except IndexError as ie:
        print(f"IndexError: {ie}")
    except Exception as e:
        print(f"Сталася непередбачувана помилка: {e}")

data = {10: 2, 2: 5, "123": 4, 18: 0, (): 15, 8: 4}
for key in data:
    res = dilen(key, data[key])
    result.append(res)

print(result)
